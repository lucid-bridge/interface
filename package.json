{
  "name": "bridge-frontend",
  "version": "0.1.0",
  "description": "Frontend for decentralized cross layer 2 bridge",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run compile-contract-types && next build",
    "start": "next start",
    "lint": "next lint",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './contracts/types' './contracts/*.json'",
    "postinstall": "npm run compile-contract-types"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/ubuntu": "^4.5.10",
    "@uauth/js": "^2.0.0",
    "@uauth/web3modal": "^2.0.0",
    "@unstoppabledomains/resolution": "^7.1.4",
    "formik": "^2.2.9",
    "framer-motion": "^6.2.8",
    "next": "12.1.0",
    "next-redux-wrapper": "^7.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "swr": "^1.3.0",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "typechain": "^8.1.0",
    "typescript": "4.5.5"
  }
}
